-- Scripts:

local function AEAWTN_fake_script() -- Frame.Main 
	local script = Instance.new('LocalScript', Frame)

	local player
	
	
	
	--frame variables
	local f = script.Parent --
	local ping = f.ping --
	local yr =f.year --
	local age = f.age --
	local display = f.display --
	local loc = f.country
	local user = f.user --
	local id = f.id --
	local pfp = f.pfp --
	local h = f.countryhelp --
	
	--game variables
	
	local localizationService = game:GetService("LocalizationService")
	--\game variables
	
	--code
	
	wait(1)
	print("loaded!!")
	player = game.Players.LocalPlayer
	local playerage = player.AccountAge
	local name = player.Name
	local playeryear = math.round((playerage/375))
	local playerid = player.UserId
	
	--img
	local thumbType = Enum.ThumbnailType.AvatarBust
	local thumbSize = Enum.ThumbnailSize.Size150x150
	local content = game.Players:GetUserThumbnailAsync(playerid, thumbType, thumbSize)
	--\img
	
	
	age.Text = "Age: "..playerage
	yr.Text = "Years: "..playeryear
	user.Text = "Username: "..name
	display.Text = "Display: "..player.DisplayName
	loc.Text = "Country: "..localizationService:GetCountryRegionForPlayerAsync(player)
	id.Text = "UserId: "..playerid
	pfp.Image = content
	
	
	h.MouseButton1Up:Connect(function()
		f.Parent.help.Visible = true
	end)
	
	game.ReplicatedStorage.OpenWAI.OnClientEvent:Connect(function()
		f.Visible = true
	end)
	
	--\code
	
	user.Changed:Connect(function()
		player = game.Players:FindFirstChild(user.Text)
		local playerage = player.AccountAge
		local name = player.Name
		local playeryear = math.round((playerage/375))
		local playerid = player.UserId
		local thumbType = Enum.ThumbnailType.AvatarBust
		local thumbSize = Enum.ThumbnailSize.Size150x150
		local content = game.Players:GetUserThumbnailAsync(playerid, thumbType, thumbSize)
		age.Text = "Age: "..playerage
		yr.Text = "Years: "..playeryear
		user.Text = "Username: "..name
		display.Text = "Display: "..player.DisplayName
		loc.Text = "Country: "..localizationService:GetCountryRegionForPlayerAsync(player)
		id.Text = "UserId: "..playerid
		pfp.Image = content
	end)
end
coroutine.wrap(AEAWTN_fake_script)()
local function KBYM_fake_script() -- Frame.Ping 
	local script = Instance.new('LocalScript', Frame)

	local player;
	local p = script.Parent.ping
	local localizationService = game:GetService("LocalizationService")
	
	wait(3)
	
	player = game.Players.LocalPlayer
	
	
	local ping = player:GetNetworkPing()
	p.Text = "$WAIT"
	
	script.Parent.user.Changed:Connect(function()
		player = game.Players:FindFirstChild(script.Parent.user.Text)
	end)
	
	while wait(1) do
		ping = player:GetNetworkPing()
		p.Text = "Ping: "..tostring(ping)
	end
	
end
coroutine.wrap(KBYM_fake_script)()
local function MNMX_fake_script() -- TextButton.close 
	local script = Instance.new('LocalScript', TextButton)

	local b = script.Parent
	local f = b.Parent
	
	b.MouseButton1Up:Connect(function()
		f.Parent:Destroy()
	end)
end
coroutine.wrap(MNMX_fake_script)()
local function KITG_fake_script() -- mini.LocalScript 
	local script = Instance.new('LocalScript', mini)

	script.Parent.MouseButton1Up:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.mini.Visible = true
	end)
end
coroutine.wrap(KITG_fake_script)()
local function PWKA_fake_script() -- help.LocalScript 
	local script = Instance.new('LocalScript', help)

	local position = 1
	
	--pages
	local p1 = script.Parent.p1
	local p2 = script.Parent.p2
	local p3 = script.Parent.p3
	
	--buttons
	local skip = script.Parent.nnext
	local prev = script.Parent.prev
	local close = script.Parent.close
	
	--close function
	close.MouseButton1Up:Connect(function()
		script.Parent.Visible = false
	end)
	
	--update page
	local function changePage()
		if position == 1 then
			p1.Visible = true
			if p2.Visible == true then
				p2.Visible = false
			end
			if p3.Visible == true then
				p3.Visible = false
			end
		elseif position == 2 then
			p2.Visible = true
			if p1.Visible == true then
				p1.Visible = false
			end
			if p3.Visible == true then
				p3.Visible = false
			end
		elseif position == 3 then
			p3.Visible = true
			if p1.Visible == true then
				p1.Visible = false
			end
			if p2.Visible == true then
				p2.Visible = false
			end
		end
	end
	
	--cycle
	
	skip.MouseButton1Up:Connect(function()
		if position == 3 then
			position = 1
		elseif position == 2 then
			position = 3
		elseif position == 1 then
			position = 2
		else
			warn("cycle error(non-fatal)")
			position = 1
		end
		changePage()
	end)
	
	prev.MouseButton1Up:Connect(function()
		if position == 3 then
			position = 2
		elseif position == 2 then
			position = 1
		elseif position == 1 then
			position = 3
		else
			warn("cycle error(non-fatal)")
			position = 1
		end
		changePage()
	end)
	
end
coroutine.wrap(PWKA_fake_script)()
local function AQWDHS_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Up:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.Frame.Visible = true
	end)
end
coroutine.wrap(AQWDHS_fake_script)()
local function ZRNMU_fake_script() -- mini_2.DragScript 
	local script = Instance.new('LocalScript', mini_2)

	--Not made by me, check out this video: https://www.youtube.com/watch?v=z25nyNBG7Js&t=22s
	--Put this inside of your Frame and configure the speed if you would like.
	--Enjoy! Credits go to: https://www.youtube.com/watch?v=z25nyNBG7Js&t=22s
	
	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
	
end
coroutine.wrap(ZRNMU_fake_script)()
